{
  "name": "accent-bridge-2",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "build": "next build",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "lint": "eslint . --ext .ts,.tsx",
    "preserve": "yarn run build",
    "serve": "serve out",
    "presync": "yarn run build",
    "sync": "cap sync",
    "android": "cap run android",
    "ios": "cap run ios",
    "pages:build": "yarn run next-on-pages",
    "pages:deploy": "yarn run pages:build && wrangler pages deploy"
  },
  "dependencies": {
    "@capacitor-community/stripe": "5",
    "@capacitor/android": "5.7.2",
    "@capacitor/core": "5.7.2",
    "@capacitor/ios": "5.7.2",
    "@capacitor/status-bar": "5.0.7",
    "@ionic/react": "7.7.4",
    "@ionic/react-router": "7.7.4",
    "@supabase/auth-ui-react": "0.4.7",
    "@supabase/auth-ui-shared": "0.1.8",
    "@supabase/ssr": "0.4.0",
    "@supabase/supabase-js": "2.44.3",
    "classnames": "2.5.1",
    "mobx": "6.13.0",
    "mobx-react-lite": "4.0.7",
    "next": "14.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-virtuoso": "4.7.1",
    "stripe": "16.4.0",
    "stripe-pwa-elements": "2.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "5.7.2",
    "@cloudflare/next-on-pages": "1.12.1",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.25",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "autoprefixer": "10.4.18",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "ionicons": "7.2.2",
    "postcss": "8.4.35",
    "pullstate": "1.25",
    "reselect": "5.1.0",
    "serve": "14.2.3",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.2",
    "vercel": "34.4.0",
    "wrangler": "3.64.0"
  }
}
